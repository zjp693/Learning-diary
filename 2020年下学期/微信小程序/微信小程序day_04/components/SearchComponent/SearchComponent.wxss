/* components/SearchComponent/SearchComponent.wxss */
@import "/styles/iconfont.wxss";

.search_input {
  position: fixed;
  z-index: 1;
  top: 0;
  width: 100%;
  height: 80rpx;
  padding: 10rpx;
  background-color: var(--themeColor);
  box-sizing: border-box;
}

.search_text {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
  border-radius: 45rpx;
  color: #666;
}